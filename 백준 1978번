// 백준 1978번 소수 찾기

#include <iostream>
#include <cassert>
using namespace std;

int main()
{
	int N;
	cin >> N;
	assert(N <= 100 && N >= 0);

	int num = 0;

	for (int i = 0; i < N; i++)
	{
		int n;
		cin >> n;
		assert(n <= 1000 && n >= 1);

		bool prime = false;
		
		if (n == 2)
		{
			// 2는 소수에 포함
			num++;
			continue;
		}

		// 소수인지 판단
		for (int j = 2; j <= n / 2 + 1; j++)
		{
			if (n % j == 0)
			{
				// 소수가 아닌 경우
				break;
			}
			
			if (j == n / 2 + 1)
			{
				// 소수인 경우
				prime = true;
			}
		}

		if (prime)
		{
			// 소수인 경우 개수 증가
			num++;
		}
	}

	cout << num;
}
